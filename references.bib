  title = {The enigmatic Chad lineament revisited with global gravity and gravity-gradient
	fields},
  journal = {Geological Society, London, Special Publications},
  year = {2011},
  volume = {357},
  pages = {329--341},
  number = {1},
  month = jan,
  abstract = {The crustal structure of northern Africa is puzzling, large areas
	being of difficult access and concealed by the Sahara. The new global
	gravity models are of unprecedented precision and spatial resolution
	and offer a new possibility to reveal the structure of the lithosphere
	beneath the Sahara. The gravity gradients correlate better than gravity
	with geological features such as rifts, fold belts and magmatic deposits
	and intrusions. They are an ideal tool to follow geological units
	(e.g. basement units) below a stratigraphic layer of varying density
	(e.g. sediments). We focus on the Chad lineament, a 1300 km arcuate
	feature located between the west and central African rift system.
	The gravity fields show differences between the lineament and the
	west and central African rift system. Along the centre of the lineament
	high-density rocks must be present, which relate to either magmatic
	or metamorphic rocks. This is very different to the lineaments of
	the western and central-west African rift system which are filled
	with sediments. Considering present models of rifting and the absence
	of topography, the lineament cannot be coeval to the west and central
	African rift system and is most likely older. We suggest that the
	lineament is a structural element of the Saharan Metacraton.},
  doi = {10.1144/SP357.18},
  file = {Full Text PDF:/home/leo/Copy/bib/zotero/storage/D96HI694/Braitenberg et al. - 2011 - The enigmatic Chad lineament revisited with global.pdf:application/pdf;Snapshot:/home/leo/Copy/bib/zotero/storage/UMW6S9X6/329.html:text/html},
  issn = {0305-8719, 2041-4927},
  language = {en},
  url = {http://sp.lyellcollection.org/content/357/1/329},
  urldate = {2013-09-18}
}

